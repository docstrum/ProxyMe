@model IEnumerable<InstaSharp.Models.Media>

@{
    ViewBag.Title = "My Feed";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>My Feed</title>
</head>
<body>


    @foreach (var item in Model)
    {
        if (item.Location != null)
        {
            <div>
                <center>
                    <img src="@item.Images.StandardResolution.Url" />
                    <br />
                    @item.User.Username - @item.User.FullName - @item.Location.Name
                    <br />
                    (@item.Location.Latitude Latitude) - (@item.Location.Longitude Longitude)
                    <br />
                    @item.CreatedTime.AddHours(-4);
                </center>
            </div>
        }
        else
        {
            <div>
                <center>
                    <img src="@item.Images.StandardResolution.Url" />
                    @item.User.Username - @item.User.FullName
                    <br />
                    @item.CreatedTime.AddHours(-4);
                </center>
            </div>
        }

    }

</body>
</html>
